# Dispatch Bridge Configuration
# Copy to .env and adjust as needed

# Dispatch service URL
DISPATCH_API_URL=http://localhost:8081

# Bridge control API port
DISPATCH_BRIDGE_PORT=8082

# AI model for fleet commander
DISPATCH_MODEL=anthropic/claude-sonnet-4-6

# Poll interval (seconds)
DISPATCH_POLL_INTERVAL=2

# OpenClaw gateway (auto-detected if running locally)
# OPENCLAW_GATEWAY_WS_URL=ws://127.0.0.1:18789
# OPENCLAW_GATEWAY_TOKEN=your-token-here
